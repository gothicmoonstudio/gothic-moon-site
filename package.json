{
  "name": "gothic-moon-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "frontend:dev": "cd apps/frontend && next dev",
    "frontend:build": "cd apps/frontend && next build",
    "frontend:start": "cd apps/frontend && next start",
    "frontend:lint": "cd apps/frontend && eslint .",
    "backend:dev": "vercel dev",
    "backend:build": "vercel build"
  },
  "dependencies": {
    "@swc/core": "^1.7.26",
    "@swc/helpers": "^0.5.13",
    "animejs": "^3.2.2",
    "emailjs-com": "^3.2.0",
    "feather-icons-react": "^0.7.0",
    "framer-motion": "^11.5.6",
    "gsap": "^3.12.5",
    "lottie-web-light": "^1.1.0",
    "next": "^14.2.14",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-feather": "^2.0.10",
    "react-scroll-parallax": "^3.4.5",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.12",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5"
  },
  "packageManager": "npm@10.8.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
