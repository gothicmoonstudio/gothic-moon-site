{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/apps/frontend/api/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/apps/frontend/$1"
      }
    ],
    "functions": {
      "apps/frontend/api/**/*.js": {
        "memory": 1024,
        "maxDuration": 30
      }
    },
    "cleanUrls": true
  }